{
  "name": "@theia/debug-java",
  "version": "0.3.16",
  "description": "Theia - Java Debug Extension",
  "dependencies": {
    "@theia/debug": "^0.3.16",
    "@theia/java": "^0.3.16",
    "ps-list": "5.0.1",
    "unzip-stream": "^0.3.0",
    "vscode-debugprotocol": "^1.32.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "theiaExtensions": [
    {
      "backend": "lib/node/debug-java-backend-module"
    }
  ],
  "keywords": [
    "theia-extension, debug, java"
  ],
  "license": "EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/theia-ide/theia.git"
  },
  "bugs": {
    "url": "https://github.com/theia-ide/theia/issues"
  },
  "homepage": "https://github.com/theia-ide/theia",
  "files": [
    "lib",
    "src",
    "scripts"
  ],
  "scripts": {
    "postinstall": "node ./scripts/download-java-extension.js",
    "prepare": "yarn run clean && yarn run build",
    "clean": "theiaext clean",
    "build": "theiaext build",
    "watch": "theiaext watch",
    "test": "theiaext test",
    "docs": "theiaext docs"
  },
  "devDependencies": {
  },
  "adapters": {
  },
  "javaExtension": {
    "downloadUrl": "https://dl.bintray.com/vscjavaci/javadebug/releases/0.15.0/plugins/com.microsoft.java.debug.plugin_0.15.0.jar"
